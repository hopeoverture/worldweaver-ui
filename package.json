{
  "name": "worldweaver-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "kill-port 3000 3001 54321 5432 8000 || true",
    "dev": "node -r ./scripts/patch-fs-readlink.cjs ./node_modules/next/dist/bin/next dev",
    "dev:clean": "npm run predev && npm run dev",
    "dev:all": "concurrently -k -n \"WEB,SEED\" \"npm run dev\" \"wait-on http://localhost:3000 && npm run seed:core\"",
    "build": "node -r ./scripts/patch-fs-readlink.cjs ./node_modules/next/dist/bin/next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "node -r ./scripts/patch-fs-readlink.cjs ./node_modules/next/dist/bin/next start",
    "test": "vitest run",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "seed:core": "node scripts/seed-core-templates.js",
    "test:api": "node scripts/test-api-endpoints.js",
    "test:mcp": "node scripts/test-mcp.js",
    "kill-ports": "kill-port 3000 3001 54321 5432 8000"
  },
  "dependencies": {
    "@modelcontextprotocol/server-filesystem": "^2025.8.21",
    "@modelcontextprotocol/server-memory": "^2025.8.4",
    "@modelcontextprotocol/server-sequential-thinking": "^2025.7.1",
    "@supabase/mcp-server-supabase": "^0.5.3",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.2",
    "@tanstack/react-query": "^5",
    "@types/pg": "^8.15.5",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.2",
    "glob": "^11.0.3",
    "mcp-supabase": "^0.1.0",
    "next": "^15.5.2",
    "openai": "^5.20.2",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "server-only": "^0.0.1",
    "tsx": "^4.20.5",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.3",
    "@next/eslint-plugin-next": "^15.5.2",
    "@tailwindcss/postcss": "^4",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.19.13",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "kill-port": "^2.0.1",
    "pg": "^8.16.3",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "uuid": "^12.0.0",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  }
}
